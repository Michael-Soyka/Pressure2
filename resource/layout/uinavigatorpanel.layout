			{
				0="fill(x0-40,y1-1,x1,y1, color__divider__solid)"
			}
		}
		
			LibrarySearch:selected {}
			
			LibrarySearch:empty
			{
				font-style=italic
				textcolor=color__disabled
			}

			LibrarySearch:empty:hover
			{
				textcolor=color__secondaryText
